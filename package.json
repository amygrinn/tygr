{
  "name": "tygr",
  "version": "1.0.0",
  "description": "Home page for Tyler Grinn",
  "main": "src/index.ts",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "serve": "http-server dist --silent",
    "clean": "rimraf reports",
    "wdio": "wdio wdio.conf.js",
    "wdio:o": "wdio repl chrome",
    "test": "npm-run-all --silent clean --parallel serve wdio --race",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "npm run lint:fix",
    "prettier:check": "prettier -c .",
    "prettier:fix": "prettier --write .",
    "prettier": "npm run prettier:fix",
    "quality:check": "run-s --print-label --silent lint:check prettier:check",
    "quality:fix": "run-p --print-label --silent lint:fix prettier:fix",
    "quality": "npm run quality:fix",
    "updateBrowserslist": "npx browserslist@latest --update-db"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylergrinn/tygr.git"
  },
  "author": "Tyler Grinn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tylergrinn/tygr/issues"
  },
  "homepage": "https://github.com/tylergrinn/tygr#readme",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "@wdio/cli": "^6.5.2",
    "@wdio/local-runner": "^6.5.2",
    "@wdio/mocha-framework": "^6.5.0",
    "@wdio/spec-reporter": "^6.4.7",
    "babel-eslint": "^10.1.0",
    "chromedriver": "^85.0.1",
    "eslint": "^7.9.0",
    "eslint-config-alloy": "^3.8.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-react": "^7.21.2",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "rimraf": "^3.0.2",
    "wdio-chromedriver-service": "^6.0.4",
    "wdio-timeline-reporter": "^5.1.4",
    "webpack-dev-server": "^3.11.0"
  }
